<project name="auditLog" default="all" basedir=".">

    <property environment="ENV"/>
    <property file="../../build.properties" />
    <import file="${development.base}/base-build.xml"/>

    <target name="all" depends="clean" >
        <if><equals arg1="${org.jboss.processFlow.provision.bpmCore}" arg2="true" />
          <then>
            <antcall target="jar" >
                <param name="jar.name" value="processFlow-${ant.project.name}" />
            </antcall>
          </then>
          <else>
            <echo message="auditLog build .... skipping" />
        </else>
      </if>
    </target>

    <target name="clean"><delete dir="${build.dir}" /></target>

</project>
