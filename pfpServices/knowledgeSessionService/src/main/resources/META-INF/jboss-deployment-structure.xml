<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.1">
    <deployment>
        <dependencies>
            <!-- meta-inf="export" required to allow hibernate visibility to ORM files in jBPM static modules -->
            <module name="org.jboss.processFlow" export="true" meta-inf="export" />

            <module name="org.codehaus.jackson.jackson-mapper-asl" />
        </dependencies>
    </deployment>

</jboss-deployment-structure>
